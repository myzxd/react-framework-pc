{
  "private": true,
  "name": "aoaoBoss",
  "version": "11.1.0",
  "scripts": {
    "start": "rm -rf ./public/config.js && cp ./config/config.dev.js ./public/config.js && cross-env NODE_ENV=development webpack serve",
    "start-prod": "rm -rf ./public/config.js && cp ./config/config.prod.js ./public/config.js && cross-env NODE_ENV=development webpack serve",
    "build-dev": "cross-env NODE_ENV=production --max_old_space_size=4096 webpack",
    "deploy-dev": "yarn build-dev && yarn send-dev && yarn push",
    "deploy-qa": "yarn build-dev && yarn send-qa && yarn push",
    "deploy-code": "yarn build-dev && yarn send-code && yarn push",
    "deploy-xingDaCode": "yarn build-dev && yarn send-xingDaCode && yarn push",
    "deploy-commonCode": "yarn build-dev && yarn send-commonCode && yarn push",
    "deploy-oa": "yarn build-dev && yarn send-oa && yarn push",
    "deploy-2081": "yarn build-dev && yarn send-2081 && yarn push",
    "send-dev": "bash ./deploy.sh dev",
    "send-qa": "bash ./deploy.sh qa",
    "send-code": "bash ./deploy.sh code",
    "send-xingDaCode": "bash ./deploy.sh xingDaCode",
    "send-commonCode": "yarn send-code && yarn send-xingDaCode",
    "send-oa": "bash ./deploy.sh oa",
    "send-2081": "bash ./deploy.sh 2081",
    "push": "git add . && git commit -am 'packup' && git push",
    "lint": "eslint --cache --fix --ext .js,jsx src test",
    "lint-staged": "lint-staged",
    "readme-boss": "bash shell.sh boss",
    "readme-oa": "bash shell.sh oa",
    "jsdoc": "jsdoc -r src/models/ src/services/ -d ./doc/source/ -c .jsdoc.conf && yarn sh-boss"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "src/**/*.js?(x)": [
      "eslint --cache --fix"
    ]
  },
  "eslintIgnore": [
    "dist/*",
    "doc/*",
    "node_modules/*"
  ],
  "dependencies": {
    "@ant-design/compatible": "^1.0.8",
    "@ant-design/icons": "^4.7.0",
    "antd": "^4.16.13",
    "countly-sdk-web": "^20.11.3",
    "cross-env": "^7.0.3",
    "crypto-js": "^3.1.9-1",
    "dot-prop": "git+ssh://git@github.com:o3cloud/dot-prop.git#v4.1.2",
    "dva": "^2.4.1",
    "dva-loading": "2.0.6",
    "immutability-helper": "^3.0.2",
    "invariant": "^2.2.2",
    "is_js": "^0.9.0",
    "js-base64": "^2.4.0",
    "js-export-excel": "^1.1.4",
    "lint-staged": "^10.1.3",
    "lodash": "^4.17.4",
    "object-mapper": "^4.2.0",
    "print-js": "^1.0.55",
    "prop-types": "^15.7.2",
    "query-string": "^6.12.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-file-viewer": "^1.2.1",
    "react-is": "^16.13.1",
    "react-responsive-masonry": "^2.1.1",
    "xlsx": "^0.11.8"
  },
  "devDependencies": {
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-proposal-decorators": "^7.15.8",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-react": "^7.14.5",
    "autoprefixer": "^10.3.7",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^8.2.3",
    "babel-plugin-dva-hmr": "^0.2.1",
    "babel-plugin-import": "^1.1.1",
    "copy-webpack-plugin": "^9.0.1",
    "css-loader": "^6.4.0",
    "css-minimizer-webpack-plugin": "^3.1.1",
    "eslint": "^3.12.2",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^2.2.3",
    "eslint-plugin-react": "^6.8.0",
    "eslint-plugin-react-hooks": "^3.0.0",
    "html-loader": "^3.0.0",
    "html-webpack-plugin": "^5.5.0",
    "husky": "^7.0.4",
    "js-yaml": "^3.12.0",
    "less": "^4.1.2",
    "less-loader": "^10.2.0",
    "markdown-loader": "^6.0.0",
    "mini-css-extract-plugin": "^2.4.3",
    "postcss-loader": "^6.2.0",
    "qs": "^6.2.1",
    "raw-loader": "^4.0.2",
    "redbox-react": "^1.3.2",
    "style-loader": "^3.3.1",
    "webpack": "^5.60.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.1",
    "webpack-dev-server": "^4.3.1",
    "webpack-merge": "^5.8.0",
    "webpackbar": "^5.0.0-3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
